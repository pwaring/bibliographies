@INPROCEEDINGS{Heer2005,
  author = {Heer, Jeffrey and Boyd, Danah},
  title = {Vizster: Visualizing Online Social Networks},
  booktitle = {Proceedings of the 2005 IEEE Symposium on Information Visualization
	(INFOVIS'05)},
  year = {2005},
  owner = {waringp3},
  review = {Not sure that I agree with the assertation of other services being
	labelled as 'YASNS' (Yet Another Social Networking Service) - this
	seems like a sweeping statement by the author with, incidentally,
	no citation to back it up.
	
	
	The point about web interfaces to social networks only showing a linear
	list of friends rather than a proper network (probably best represented
	as a graph with nodes for individuals and vertices representing connections
	such as colleagues or friends) is a valid one, though I am not sure
